{
    "comments": {
    },
    "brackets": [ ["{", "}"] ],
    "autoClosingPairs": [
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "<",
            "close": ">"
        },
        {
            "open": "\"",
            "close": "\"",
            "notIn": [
                "string"
            ]
        }
    ],
    "autoCloseBefore": ";:.,=}])>` \n\t",
    "surroundingPairs": [
        [
            "{",
            "}"
        ],
        [
            "\"",
            "\""
        ]
    ],
    "folding": {
    },
    "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
    "indentationRules": {
        "increaseIndentPattern": "^.*(\\{[^\\n}\"]*)$",
        "decreaseIndentPattern": "^\\s*\\}.*$"
    }
}